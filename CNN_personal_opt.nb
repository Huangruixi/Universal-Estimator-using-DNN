Cell[BoxData[
 RowBox[{
  RowBox[{"GenerateData", "[", 
   RowBox[{"M_", ",", " ", "n_", ",", " ", "f_", ",", " ", "range_", ",", " ", 
    RowBox[{"bins_", ":", 
     RowBox[{"-", "1"}]}]}], "]"}], ":=", "\n", "\t", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "params", ",", " ", "samples", ",", " ", "nbins", ",", " ", "MAXBINS"}], 
     "}"}], ",", "\n", "\t\t", 
    RowBox[{
     RowBox[{"params", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"RandomReal", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"range", "[", 
            RowBox[{"[", "1", "]"}], "]"}], ",", 
           RowBox[{"range", "[", 
            RowBox[{"[", "2", "]"}], "]"}]}], "}"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "M"}], "}"}]}], "]"}]}], ";", "\n", 
     "\t\t", 
     RowBox[{"(*", 
      RowBox[{
      "generate", " ", "M", " ", "different", " ", "random", " ", 
       "initparameters", " ", "in", " ", "the", " ", 
       RowBox[{"range", " ", "[", 
        RowBox[{"min_\[Theta]", ",", "max_\[Theta]"}], "]"}]}], "*)"}], " ", 
     "\n", "\t\t", 
     RowBox[{"samples", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"f", "[", 
         RowBox[{
          RowBox[{"params", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", "n"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "M"}], "}"}]}], "]"}]}], ";", "\n", 
     "\t\t", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"generate", " ", "M", " ", "distributions"}], ",", " ", 
       RowBox[{"each", " ", "distribution", " ", "has", " ", "n", " ", 
        RowBox[{"number", "/", "data"}], "*"}], ",", " ", 
       RowBox[{
       "now", " ", "we", " ", "have", " ", "M", "*", "n", " ", "data"}]}], 
      "*)"}], "\n", "\t\t", 
     RowBox[{"MAXBINS", "=", "5000"}], ";", "\n", "\t\t", 
     RowBox[{"nbins", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"bins", ">", "0"}], ",", "bins", ",", 
        RowBox[{"Min", "[", 
         RowBox[{"MAXBINS", ",", " ", 
          RowBox[{
           RowBox[{"Ceiling", "[", 
            RowBox[{"Max", "[", "samples", "]"}], "]"}], "+", "1"}]}], 
         "]"}]}], 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Ceiling", "[", 
            RowBox[{"Max", "[", "samples", "]"}], "]"}], "+", "2"}], "]"}], 
         " ", "if", " ", "using", " ", "zero", " ", "padding"}], "*)"}], 
       "]"}]}], ";", "\n", "\t\t", 
     RowBox[{
      RowBox[{"h", "[", "x_", "]"}], ":=", 
      RowBox[{
       RowBox[{"HistogramList", "[", 
        RowBox[{"x", ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "nbins", ",", "1"}], "}"}]}], "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"without", " ", "using", " ", "log10", 
       RowBox[{"(", 
        RowBox[{"x", "+", "1"}], ")"}]}], "*)"}], "\n", "\t\t", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"h", "@", 
         RowBox[{"samples", "[", 
          RowBox[{"[", "i", "]"}], "]"}]}], "->", 
        RowBox[{"params", "[", 
         RowBox[{"[", "i", "]"}], "]"}]}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "M"}], "}"}]}], "]"}]}]}], "  ", "\n", 
   "\t", "\n", "\t", "]"}]}]], "Code",
