Cell[BoxData[
 RowBox[{
  RowBox[{"NewAdjuster", "[", 
   RowBox[{"low_", ",", " ", "high_", ",", "a_"}], "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"LOW", "=", 
       RowBox[{"NewSigmoid", "[", 
        RowBox[{"low", ",", "a"}], "]"}]}], ",", 
      RowBox[{"HIGH", "=", 
       RowBox[{"NewSigmoid", "[", 
        RowBox[{"high", ",", "a"}], "]"}]}]}], "}"}], ",", " ", 
    RowBox[{"Function", "[", 
