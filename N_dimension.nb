Cell[BoxData[
 RowBox[{
  RowBox[{"GenerateData", "[", 
   RowBox[{
   "M_", ",", " ", "n_", ",", " ", "f_", ",", " ", "ranges_", ",", " ", "a_", 
    ",", " ", 
    RowBox[{"bins_", ":", 
     RowBox[{"-", "1"}]}]}], "]"}], ":=", " ", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "nParams", ",", " ", "adjusters", ",", " ", "params", ",", "samples", 
      ",", " ", "nbins"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "a", " ", "helper", " ", "function", " ", "for", " ", "1", "d", " ", 
      "dimension", " ", "case", " ", "if", " ", "needed"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"Params", "[", "params_", "]"}], ":=", " ", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Length", "[", "params", "]"}], ">", "1"}], ",", " ", 
        "params", ",", " ", 
        RowBox[{"params", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "number", " ", "of", " ", "group", " ", "of", " ", "parameters"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"nParams", "=", 
      RowBox[{
       RowBox[{"Dimensions", "[", "ranges", "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "generate", " ", "an", " ", "adapter", " ", "for", " ", "each", " ", 
       "parameter", " ", "range"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"adjusters", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"NewAdjuster", "[", 
         RowBox[{
          RowBox[{"ranges", "[", 
           RowBox[{"[", 
            RowBox[{"i", ",", "1"}], "]"}], "]"}], ",", 
          RowBox[{"ranges", "[", 
           RowBox[{"[", 
            RowBox[{"i", ",", "2"}], "]"}], "]"}], ",", " ", 
          RowBox[{"a", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "nParams"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "generate", " ", "N", " ", "parameters", " ", "cube", " ", "in", " ", 
       "the", " ", "range", " ", 
       RowBox[{"(", 
        RowBox[{"0", ",", "1"}], ")"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"params", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"adjusters", "[", 
          RowBox[{"[", "j", "]"}], "]"}], "[", 
         RowBox[{"RandomReal", "[", 
          RowBox[{"{", 
           RowBox[{"0", ",", "1"}], "}"}], "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "M"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "nParams"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "generate", " ", "M", " ", "samples", " ", "from", " ", "the", " ", 
       "parameters"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"samples", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"f", "[", 
         RowBox[{
          RowBox[{"Params", "@", 
           RowBox[{"params", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], ",", "n"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "M"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "create", " ", "a", " ", "histogram", " ", "from", " ", "each", " ", 
       "sample"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"nbins", "=", 
      RowBox[{"If", " ", "[", 
       RowBox[{
        RowBox[{"bins", ">", "0"}], ",", "bins", ",", 
        RowBox[{
         RowBox[{"Max", "[", "samples", "]"}], "+", "1"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"h", "[", "x_", "]"}], ":=", 
      RowBox[{
       RowBox[{"HistogramList", "[", 
        RowBox[{"x", ",", " ", 
         RowBox[{"{", 
          RowBox[{"0", ",", "nbins", ",", "1"}], "}"}]}], "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", "output", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"h", "@", 
         RowBox[{"samples", "[", 
          RowBox[{"[", "i", "]"}], "]"}]}], "->", 
        RowBox[{"Params", "@", " ", 
         RowBox[{"params", "[", 
          RowBox[{"[", "i", "]"}], "]"}]}]}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"i", ",", " ", "1", ",", " ", "M"}], "}"}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Code",
