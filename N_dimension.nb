Cell[BoxData[
 RowBox[{
  RowBox[{"RunExperiment", "[", 
   RowBox[{"dist_", ",", " ", "range_", ",", " ", "a_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "estimator", ",", " ", "g", ",", " ", "adjuster", ",", " ", "params", 
      ",", " ", "n", ",", " ", "samples", ",", " ", "\[CapitalTheta]", ",", 
      " ", "\[CapitalTheta]hat", ",", " ", "e", ",", " ", "TestSet", ",", " ",
       "M", ",", " ", "crb", ",", "Var"}], "}"}], ",", "\[IndentingNewLine]", 
    
    RowBox[{"(*", 
     RowBox[{
     "generate", " ", "an", " ", "Estimator", " ", "for", " ", "dist"}], 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"estimator", " ", "=", " ", 
      RowBox[{"UniversalEstimator", "[", 
       RowBox[{"dist", ",", " ", "range", ",", " ", "a"}], "]"}]}], ";", "\n", 
     RowBox[{"(*", 
      RowBox[{
      "define", " ", "the", " ", "size", " ", "of", " ", "the", " ", "test", 
       " ", "set"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"M", " ", "=", " ", "20"}], ";", "\n", 
     RowBox[{"n", " ", "=", " ", "1024"}], ";", "\[IndentingNewLine]", "\n", 
     RowBox[{"\[CapitalTheta]", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"a", ",", "M"}], "]"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "create", " ", "M", " ", "distributions", " ", "of", " ", "parameter", 
       " ", "a"}], "*)"}], "\n", "\t", "\n", 
     RowBox[{
      RowBox[{"g", "[", 
       RowBox[{"\[Theta]_", ",", "m_"}], "]"}], " ", ":=", " ", 
      RowBox[{"RandomVariate", "[", 
       RowBox[{
        RowBox[{"TwodimDist", "[", 
         RowBox[{"dist", ",", "\[Theta]"}], "]"}], ",", "m"}], "]"}]}], ";", 
     "\n", "\[IndentingNewLine]", 
     RowBox[{"samples", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"g", "[", 
         RowBox[{
          RowBox[{"\[CapitalTheta]", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", " ", "n"}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "M", ",", "1"}], " ", "}"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "predict", " ", "the", " ", "parameter", " ", "for", " ", "each", " ", 
       "sample"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"\[CapitalTheta]hat", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"estimator", "[", 
         RowBox[{"samples", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "M", ",", "1"}], " ", "}"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Compute", " ", 
        StyleBox["Cramer",
         FontSlant->"Italic"]}], 
       StyleBox["-",
        FontSlant->"Italic"], 
       RowBox[{
        StyleBox["Rao",
         FontSlant->"Italic"], 
        StyleBox[" ",
         FontSlant->"Italic"], 
        StyleBox["bound",
         FontSlant->"Italic"], 
        StyleBox[" ",
         FontSlant->"Italic"], "at", " ", "a"}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"crb", " ", "=", " ", 
        RowBox[{"CRB", "[", 
         RowBox[{"dist", ",", " ", "a", ",", " ", "n"}], "]"}]}], ";"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"evaluate", " ", "prediction", " ", "errors"}], "*)"}], "\n", 
     RowBox[{"e", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Abs", "[", 
         RowBox[{
          RowBox[{"\[CapitalTheta]", "[", 
           RowBox[{"[", "i", "]"}], "]"}], " ", "-", " ", 
          RowBox[{"\[CapitalTheta]hat", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "M", ",", "1"}], " ", "}"}]}], "]"}]}], 
     ";", "\n", 
     RowBox[{"(*", 
      RowBox[{
      "Compute", " ", "the", " ", "varience", " ", "of", " ", "each", " ", 
       FormBox[
        SubscriptBox["\[Theta]", "i"],
        TraditionalForm]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Var", " ", "=", " ", 
        RowBox[{"Mean", "[", 
         RowBox[{"e", "^", "2"}], "]"}]}], ";"}], "*)"}], "\n", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<\[Theta]: \>\"", ",", "\[CapitalTheta]"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
      "\"\<\!\(\*OverscriptBox[\(\[Theta]\), \(^\)]\): \>\"", ",", 
       "\[CapitalTheta]hat"}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
      "\"\<abs(\[Theta]-\!\(\*OverscriptBox[\(\[Theta]\), \(^\)]\)): \>\"", 
       ",", "e"}], "]"}], ";", "\n", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", 
        RowBox[{"\"\<Variance for \[Theta]:\>\"", ",", "Var"}], "]"}], ";"}], 
      "*)"}], "\n", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", 
        RowBox[{"\"\<crb for \[Theta]:\>\"", ",", "crb"}], "]"}], ";"}], 
      "*)"}], "\n", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", 
        RowBox[{"\"\<ratio of variance and crb for \[Theta]:\>\"", ",", 
         RowBox[{"Var", "/", "crb"}]}], "]"}], ";"}], "*)"}], "\n", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", 
        RowBox[{"\"\<RMSE:\>\"", ",", 
         RowBox[{"RootMeanSquare", "[", "e", "]"}]}], "]"}], ";"}], "*)"}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "return", " ", "true", " ", "parameters", " ", "and", " ", "predicted", 
       " ", "errors"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"\[CapitalTheta]", ",", " ", "e"}], "}"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Code",
