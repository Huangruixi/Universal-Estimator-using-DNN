Cell[BoxData[
 RowBox[{
  RowBox[{"UniversalEstimator", "[", 
   RowBox[{"dist_", ",", " ", "range_", ",", " ", "a_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "g", ",", " ", "M", ",", " ", "n", ",", " ", "trainSet", ",", " ", 
      "bins", ",", " ", "valSet", " ", ",", "net", ",", " ", "trained", ",", 
      " ", "model", ",", " ", "h", ",", " ", "nParams"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"define", " ", "a", " ", "sample", " ", "generator"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"g", "[", 
       RowBox[{"\[Theta]_", ",", "n_"}], "]"}], " ", ":=", " ", 
      RowBox[{"RandomVariate", "[", 
       RowBox[{
        RowBox[{"TwodimDist", "[", 
         RowBox[{"dist", ",", "\[Theta]"}], "]"}], ",", "n"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "train", " ", "a", " ", "NN", " ", "for", " ", "predicting", " ", "the",
        " ", "parameter", " ", "\[Theta]"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "generate", " ", "learning", " ", "samples", " ", "in", " ", "the", " ",
        "the", " ", "given", " ", "parameter", " ", "range"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"M", " ", "=", " ", "10000"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"n", " ", "=", " ", "1024"}], ";", "   ", 
     RowBox[{"(*", 
      RowBox[{
      "number", " ", "of", " ", "observations", " ", "in", " ", "each", " ", 
       "sample"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"trainSet", "=", 
      RowBox[{"GenerateData", "[", 
       RowBox[{
       "M", ",", "n", ",", " ", "g", ",", " ", "range", ",", " ", "a"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"bins", " ", "=", " ", 
      RowBox[{"Length", "@", 
       RowBox[{
        RowBox[{"trainSet", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}]}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "use", " ", "the", " ", "same", " ", "number", " ", "of", " ", "bins", 
       " ", "for", " ", "the", " ", "validation", " ", "set"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"valSet", "=", 
      RowBox[{"GenerateData", "[", 
       RowBox[{
        RowBox[{"M", "/", "2"}], ",", "n", ",", " ", "g", ",", " ", "range", 
        ",", " ", "a", ",", " ", "bins"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"nParams", "=", 
      RowBox[{
       RowBox[{"Dimensions", "[", "range", "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Create", " ", "and", " ", "train", " ", "the", " ", "NN"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"net", " ", "=", " ", 
      RowBox[{"NetInitialize", "@", 
       RowBox[{"CreateNet", "[", 
        RowBox[{"bins", ",", "nParams"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"trained", "=", 
      RowBox[{"NetTrain", "[", "\[IndentingNewLine]", 
       RowBox[{
       "net", ",", "\[IndentingNewLine]", "trainSet", ",", 
        "\[IndentingNewLine]", "All", ",", "\[IndentingNewLine]", 
        RowBox[{"ValidationSet", "\[Rule]", "valSet"}], ",", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"TrainingStoppingCriterion", "->", 
         RowBox[{"<|", 
          RowBox[{
           RowBox[{"\"\<Criterion\>\"", "->", "\"\<Loss\>\""}], ",", 
           RowBox[{"\"\<Patience\>\"", "->", "500"}]}], "|>"}]}], ",", " ", 
        RowBox[{"(*", 
         RowBox[{
         "using", " ", "loss", " ", "criterion", " ", "to", " ", "prevent", 
          " ", "overfitting"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"Method", "->", "\"\<ADAM\>\""}], " ", ",", 
        "\[IndentingNewLine]", 
        RowBox[{"BatchSize", "\[Rule]", "256"}]}], "\[IndentingNewLine]", " ",
        "\n", "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", "trained", "]"}], ";", "  ", 
     RowBox[{"(*", 
      RowBox[{"print", " ", "the", " ", "training", " ", "result"}], "*)"}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"model", " ", "=", " ", 
      RowBox[{"trained", "[", "\"\<TrainedNet\>\"", "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "define", " ", "a", " ", "function", " ", "for", " ", "generating", 
        " ", "a", " ", "histogram", " ", "with", " ", "nbins", " ", "from", 
        " ", "an", " ", "input", " ", "sample"}], " ", ")"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"h", "[", "x_", "]"}], ":=", 
      RowBox[{
       RowBox[{"HistogramList", "[", 
        RowBox[{"x", ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "bins", ",", "1"}], "}"}]}], "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"return", " ", "an", " ", 
       StyleBox["Estimator",
        FontSlant->"Italic"], 
       StyleBox[" ",
        FontSlant->"Italic"], 
       StyleBox["which",
        FontSlant->"Italic"], 
       StyleBox[" ",
        FontSlant->"Italic"], 
       StyleBox["is",
        FontSlant->"Italic"], 
       StyleBox[" ",
        FontSlant->"Italic"], 
       StyleBox["a",
        FontSlant->"Italic"], 
       StyleBox[" ",
        FontSlant->"Italic"], 
       StyleBox["fucntion",
        FontSlant->"Italic"], 
       StyleBox[" ",
        FontSlant->"Italic"], 
       StyleBox["map",
        FontSlant->"Italic"], 
       StyleBox[" ",
        FontSlant->"Italic"], 
       StyleBox["sample",
        FontSlant->"Italic"], 
       StyleBox[" ",
        FontSlant->"Italic"], 
       StyleBox["to",
        FontSlant->"Italic"], 
       StyleBox[" ",
        FontSlant->"Italic"], 
       StyleBox["a",
        FontSlant->"Italic"], 
       StyleBox[" ",
        FontSlant->"Italic"], 
       StyleBox["parameter",
        FontSlant->"Italic"]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Function", "[", 
      RowBox[{
       RowBox[{"{", "sample", "}"}], ",", " ", 
       RowBox[{"(", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "return", " ", "a", " ", "prediction", " ", "of", " ", "the", " ", 
          "parameter", " ", "\[Theta]"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"model", "[", 
         RowBox[{"h", "@", "sample"}], "]"}], "\[IndentingNewLine]", ")"}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Code",
 CellChangeTimes->{
