Cell[BoxData[
 RowBox[{
  RowBox[{"FisherInformation", "[", "dist_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "f", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"let", " ", 
      StyleBox["f",
       FontSlant->"Italic"], " ", "be", " ", "the", " ", 
      StyleBox["PDF",
       FontSlant->"Italic"], " ", "of", " ", 
      StyleBox["dist",
       FontSlant->"Italic"], " ", "with", " ", "parameter", " ", "\[Theta]", 
      " ", "evaluated", " ", "at", " ", 
      StyleBox["x",
       FontSlant->"Italic"]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"f", " ", "=", " ", 
      RowBox[{"PDF", "[", 
       RowBox[{
        RowBox[{"dist", "[", "\[Theta]", "]"}], ",", "x"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Expectation", "[", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"Log", "[", "f", "]"}], ",", " ", 
          RowBox[{"{", "\[Theta]", "}"}]}], "]"}], "2"], ",", 
       RowBox[{"x", "\[Distributed]", 
        RowBox[{"dist", "[", "\[Theta]", "]"}]}]}], "]"}]}]}], "  ", 
   RowBox[{"(*", 
    RowBox[{"the", " ", "return", " ", "of", " ", "this", " ", "function"}], 
    "*)"}], "\[IndentingNewLine]", "]"}]}]], "Input",
