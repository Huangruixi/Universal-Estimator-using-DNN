Cell["\<\
Generate a DNN for predicting the parameter \[Theta] from the distribution \
histogram
The input size is the number of bins in the input histogram, the output is a \
scalar predicting the parameter used to generate the input sample.\
\>", "Text",
 CellChangeTimes->{{3.8749996578116903`*^9, 3.874999726659309*^9}, {
  3.875000046053153*^9, 3.875000052739278*^9}, {3.87527605670364*^9, 
  3.875276076405097*^9}},ExpressionUUID->"53d092b8-4546-4360-921f-\
fa3063504cce"],

Cell[BoxData[
 RowBox[{
  RowBox[{"CreateNet", "[", "bins_", "]"}], ":=", 
  RowBox[{"(", " ", "\[IndentingNewLine]", 
   RowBox[{"NetChain", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"LinearLayer", "[", "bins", "]"}], ",", "  ", 
       "\[IndentingNewLine]", 
       RowBox[{"BatchNormalizationLayer", "[", "]"}], ",", "  ", 
       RowBox[{"(*", 
        RowBox[{"batch", " ", "normaliztiozion"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"ElementwiseLayer", "[", "Ramp", "]"}], ",", " ", 
       RowBox[{"(*", 
        RowBox[{"Ramp", " ", "is", " ", "the", " ", "relu"}], "*)"}], 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"LinearLayer", "[", "bins", "]"}], ",", " ", 
       "\[IndentingNewLine]", 
       RowBox[{"BatchNormalizationLayer", "[", "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"ElementwiseLayer", "[", "Ramp", "]"}], ",", " ", 
       "\[IndentingNewLine]", "\n", 
       RowBox[{"LinearLayer", "[", "bins", "]"}], ",", " ", 
       "\[IndentingNewLine]", 
       RowBox[{"BatchNormalizationLayer", "[", "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"ElementwiseLayer", "[", "Ramp", "]"}], ",", " ", "\n", 
       "\[IndentingNewLine]", 
       RowBox[{"LinearLayer", "[", "1", "]"}]}], "\[IndentingNewLine]", "}"}],
      ",", "\[IndentingNewLine]", 
     RowBox[{"\"\<Input\>\"", "\[Rule]", "bins"}], ",", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"\"\<Output\>\"", " ", "->", " ", "\"\<Scalar\>\""}]}], "]"}], 
   "\[IndentingNewLine]", ")"}]}]], "Code",
 CellChangeTimes->{{3.8750067261684427`*^9, 3.875006762839406*^9}, {
   3.875006828325356*^9, 3.8750068749560833`*^9}, {3.8750070868693037`*^9, 
