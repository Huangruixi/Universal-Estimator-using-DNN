Cell[BoxData[
 RowBox[{
  RowBox[{"RunExperiment", "[", 
   RowBox[{"dist_", ",", " ", "range_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "estimator", ",", " ", "g", ",", " ", "adjuster", ",", " ", "params", 
      ",", " ", "n", ",", " ", "samples", ",", " ", "\[CapitalTheta]", ",", 
      " ", "\[CapitalTheta]hat", ",", " ", "e", ",", " ", "TestSet", ",", " ",
       "M", ",", " ", "crb", ",", "Var", ",", "a"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "generate", " ", "an", " ", "Estimator", " ", "for", " ", "dist"}], 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"estimator", " ", "=", " ", 
      RowBox[{"UniversalEstimator", "[", 
       RowBox[{"dist", ",", " ", "range"}], "]"}]}], ";", "\n", 
     RowBox[{"(*", 
      RowBox[{
      "define", " ", "the", " ", "size", " ", "of", " ", "the", " ", "test", 
       " ", "set"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"M", " ", "=", " ", "100"}], ";", "\n", 
     RowBox[{"n", " ", "=", " ", "1024"}], ";", "\[IndentingNewLine]", 
     RowBox[{"a", " ", "=", " ", "5"}], ";", "\n", "\n", 
     RowBox[{"\[CapitalTheta]", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"a", ",", "M"}], "]"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "create", " ", "M", " ", "distributions", " ", "of", " ", "parameter", 
       " ", "a"}], "*)"}], "\n", "\t", "\n", 
     RowBox[{
      RowBox[{"g", "[", 
       RowBox[{"\[Theta]_", ",", "m_"}], "]"}], " ", ":=", " ", 
      RowBox[{"RandomVariate", "[", 
       RowBox[{
        RowBox[{"dist", "[", "\[Theta]", "]"}], ",", "m"}], "]"}]}], ";", 
     "\n", "\[IndentingNewLine]", 
     RowBox[{"samples", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"g", "[", 
         RowBox[{
          RowBox[{"\[CapitalTheta]", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", " ", "n"}], "]"}], ",", " ", 
        RowBox[{"{", 
