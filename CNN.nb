       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"return", " ", "an", " ", 
       StyleBox["Estimator",
        FontSlant->"Italic"], 
       StyleBox[" ",
        FontSlant->"Italic"], 
       StyleBox["which",
        FontSlant->"Italic"], 
       StyleBox[" ",
        FontSlant->"Italic"], 
