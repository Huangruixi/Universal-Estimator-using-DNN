Cell[BoxData[
 RowBox[{
  RowBox[{"RunExperiment", "[", 
   RowBox[{"dist_", ",", " ", "range_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "estimator", ",", " ", "g", ",", " ", "adjuster", ",", " ", "params", 
      ",", " ", "n", ",", " ", "samples", ",", " ", "\[CapitalTheta]", ",", 
      " ", "\[CapitalTheta]hat", ",", " ", "e", ",", " ", "TestSet", ",", " ",
       "M", ",", " ", "init\[CapitalTheta]", ",", " ", "expansion", ",", 
      "S"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "generate", " ", "an", " ", "Estimator", " ", "for", " ", "dist"}], 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"estimator", " ", "=", " ", 
      RowBox[{"UniversalEstimator", "[", 
       RowBox[{"dist", ",", " ", "range"}], "]"}]}], ";", "\n", 
     RowBox[{"(*", 
      RowBox[{
      "define", " ", "the", " ", "size", " ", "of", " ", "the", " ", "test", 
       " ", "set"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"M", " ", "=", " ", "20"}], ";", "\n", 
     RowBox[{"S", " ", "=", " ", "5"}]}], "\:ff1b", "\n", 
    RowBox[{
     RowBox[{"n", " ", "=", " ", "1024"}], ";", "\[IndentingNewLine]", "\n", 
     RowBox[{"init\[CapitalTheta]", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"RandomReal", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"range", "[", 
            RowBox[{"[", "1", "]"}], "]"}], ",", 
           RowBox[{"range", "[", 
            RowBox[{"[", "2", "]"}], "]"}]}], "}"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "M"}], "}"}]}], "]"}]}], ";", "\n", 
     RowBox[{"(*", 
      RowBox[{
      "generate", " ", "M", " ", "different", " ", "random", " ", 
       "init\[CapitalTheta]", " ", "in", " ", "the", " ", 
       RowBox[{"range", " ", "[", 
        RowBox[{"min_\[Theta]", ",", "max_\[Theta]"}], "]"}]}], "*)"}], "\n", 
     
     RowBox[{
      RowBox[{"expansion", "[", "j_", "]"}], ":=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"init\[CapitalTheta]", "[", 
         RowBox[{"[", "j", "]"}], "]"}], ",", "S"}], "]"}]}], ";", " ", "\n", 
     
     RowBox[{"(*", 
      RowBox[{
      "define", " ", "an", " ", "expansion", " ", "function", " ", "for", " ",
        "duplicating", " ", "the", " ", "init\[CapitalTheta]"}], "*)"}], "\n", 
     RowBox[{"\[CapitalTheta]", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"expansion", "[", "w", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"w", ",", "1", ",", "M"}], "}"}]}], "]"}], "]"}]}], ";", 
     "\n", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "increase", " ", "the", " ", "number", " ", "of", " ", "each", " ", 
        "different", " ", "parameters", " ", "to", " ", "S"}], ",", " ", 
       RowBox[{
       "now", " ", "we", " ", "have", " ", "M", "*", "S", " ", 
        "\[CapitalTheta]"}]}], " ", "*)"}], "\t\t", "\n", 
     RowBox[{
      RowBox[{"g", "[", 
       RowBox[{"\[Theta]_", ",", "a_"}], "]"}], " ", ":=", " ", 
      RowBox[{"RandomVariate", "[", 
       RowBox[{
        RowBox[{"dist", "[", "\[Theta]", "]"}], ",", "a"}], "]"}]}], ";", 
     "\n", 
     RowBox[{"(*", 
      RowBox[{"create", " ", "M", "*", "S", " ", "test", " ", "samples"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"samples", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"g", "[", 
         RowBox[{
          RowBox[{"\[CapitalTheta]", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", " ", "n"}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"M", "*", "S"}], ",", "1"}], " ", "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "predict", " ", "the", " ", "parameter", " ", "for", " ", "each", " ", 
       "sample"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"\[CapitalTheta]hat", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"estimator", "[", 
         RowBox[{"samples", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"M", "*", "S"}], ",", "1"}], " ", "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\n", 
     RowBox[{"(*", 
      RowBox[{"evaluate", " ", "prediction", " ", "errors"}], "*)"}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"e", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Abs", "[", 
         RowBox[{
          RowBox[{"\[CapitalTheta]", "[", 
           RowBox[{"[", "i", "]"}], "]"}], " ", "-", " ", 
          RowBox[{"\[CapitalTheta]hat", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"M", "*", "S"}], ",", "1"}], " ", "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<\[Theta]: \>\"", ",", "\[CapitalTheta]"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
      "\"\<\!\(\*OverscriptBox[\(\[Theta]\), \(^\)]\): \>\"", ",", 
       "\[CapitalTheta]hat"}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
      "\"\<abs(\[Theta]-\!\(\*OverscriptBox[\(\[Theta]\), \(^\)]\)): \>\"", 
       ",", "e"}], "]"}], ";", "\n", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<RMSE:\>\"", ",", 
       RowBox[{"RootMeanSquare", "[", "e", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "return", " ", "true", " ", "parameters", " ", "and", " ", "predicted", 
       " ", "errors"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"\[CapitalTheta]", ",", " ", "e"}], "}"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Code",
 CellChangeTimes->{{3.875082668005588*^9, 3.875082687864887*^9}, {
