        " ", "parameter", " ", "\[Theta]"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "generate", " ", "learning", " ", "samples", " ", "in", " ", "the", " ",
        "the", " ", "given", " ", "parameter", " ", "range"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"M", " ", "=", " ", "10000"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"n", " ", "=", " ", "1024"}], ";", "   ", 
     RowBox[{"(*", 
      RowBox[{
      "number", " ", "of", " ", "observations", " ", "in", " ", "each", " ", 
       "sample"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"trainSet", "=", 
      RowBox[{"GenerateData", "[", 
       RowBox[{
       "M", ",", "n", ",", " ", "g", ",", " ", "range", ",", " ", "a"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"bins", " ", "=", " ", 
      RowBox[{"Length", "@", 
       RowBox[{
        RowBox[{"trainSet", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}]}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "use", " ", "the", " ", "same", " ", "number", " ", "of", " ", "bins", 
       " ", "for", " ", "the", " ", "validation", " ", "set"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"valSet", "=", 
      RowBox[{"GenerateData", "[", 
       RowBox[{
        RowBox[{"M", "/", "2"}], ",", "n", ",", " ", "g", ",", " ", "range", 
        ",", " ", "a", ",", " ", "bins"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Create", " ", "and", " ", "train", " ", "the", " ", "NN"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"net", " ", "=", " ", 
      RowBox[{"NetInitialize", "@", 
       RowBox[{"CreateNet", "[", "bins", "]"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"trained", "=", 
      RowBox[{"NetTrain", "[", "\[IndentingNewLine]", 
       RowBox[{
       "net", ",", "\[IndentingNewLine]", "trainSet", ",", 
        "\[IndentingNewLine]", "All", ",", "\[IndentingNewLine]", 
        RowBox[{"ValidationSet", "\[Rule]", "valSet"}], ",", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"TrainingStoppingCriterion", "->", 
         RowBox[{"<|", 
          RowBox[{
           RowBox[{"\"\<Criterion\>\"", "->", "\"\<Loss\>\""}], ",", 
           RowBox[{"\"\<Patience\>\"", "->", "500"}]}], "|>"}]}], ",", " ", 
        RowBox[{"(*", 
         RowBox[{
         "using", " ", "loss", " ", "criterion", " ", "to", " ", "prevent", 
          " ", "overfitting"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"Method", "->", "\"\<ADAM\>\""}], " ", ",", 
        "\[IndentingNewLine]", 
        RowBox[{"BatchSize", "\[Rule]", "1024"}]}], "\[IndentingNewLine]", 
       " ", "\n", "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     
     RowBox[{"Print", "[", "trained", "]"}], ";", "  ", 
     RowBox[{"(*", 
      RowBox[{"print", " ", "the", " ", "training", " ", "result"}], "*)"}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"model", " ", "=", " ", 
      RowBox[{"trained", "[", "\"\<TrainedNet\>\"", "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "define", " ", "a", " ", "function", " ", "for", " ", "generating", 
        " ", "a", " ", "histogram", " ", "with", " ", "nbins", " ", "from", 
