Cell[BoxData[
 RowBox[{
  RowBox[{"CreateNet", "[", "bins_", "]"}], ":=", 
  RowBox[{"(", " ", "\[IndentingNewLine]", 
   RowBox[{"NetChain", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"LinearLayer", "[", "bins", "]"}], ",", "  ", 
       "\[IndentingNewLine]", 
       RowBox[{"BatchNormalizationLayer", "[", "]"}], ",", "  ", 
