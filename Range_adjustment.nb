Cell[BoxData[
 RowBox[{
  RowBox[{"UniversalEstimator", "[", 
   RowBox[{"dist_", ",", " ", "range_", ",", " ", "a_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "g", ",", " ", "M", ",", " ", "n", ",", " ", "trainSet", ",", " ", 
      "bins", ",", " ", "valSet", " ", ",", "net", ",", " ", "trained", ",", 
      " ", "model", ",", " ", "h"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"define", " ", "a", " ", "sample", " ", "generator"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"g", "[", 
       RowBox[{"\[Theta]_", ",", "n_"}], "]"}], " ", ":=", " ", 
      RowBox[{"RandomVariate", "[", 
       RowBox[{
        RowBox[{"dist", "[", "\[Theta]", "]"}], ",", "n"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
