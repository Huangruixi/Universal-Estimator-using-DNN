       RowBox[{"(*", 
        RowBox[{"Ramp", " ", "is", " ", "the", " ", "relu"}], "*)"}], 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"LinearLayer", "[", "bins", "]"}], ",", " ", 
       "\[IndentingNewLine]", 
