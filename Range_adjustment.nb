       "\[IndentingNewLine]", 
       RowBox[{"ElementwiseLayer", "[", "Ramp", "]"}], ",", " ", 
       "\[IndentingNewLine]", "\n", 
       RowBox[{"LinearLayer", "[", "bins", "]"}], ",", " ", 
       "\[IndentingNewLine]", 
       RowBox[{"BatchNormalizationLayer", "[", "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"ElementwiseLayer", "[", "Ramp", "]"}], ",", " ", "\n", 
       "\[IndentingNewLine]", 
       RowBox[{"LinearLayer", "[", "1", "]"}]}], "\[IndentingNewLine]", "}"}],
      ",", "\[IndentingNewLine]", 
     RowBox[{"\"\<Input\>\"", "\[Rule]", "bins"}], ",", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"\"\<Output\>\"", " ", "->", " ", "\"\<Scalar\>\""}]}], "]"}], 
   "\[IndentingNewLine]", ")"}]}]], "Code",
